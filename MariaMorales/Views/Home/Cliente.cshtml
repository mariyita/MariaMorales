

<h1>Cliente</h1>
<div class="row">

    <div class=col-md-6>
        <form asp-action="CrearCliente" method="post">


            <div class="form-group">
                <label class=col-form-label>Nombre</label>
                <input class="form-control Nombre" type="text" name="Nombre" />
            </div>

            <div class="form-group">
                <label>Apellido</label>
                <input class="form-control Apellido" type="text" name="Apellido" />
            </div>
            <div class="form-group">
                <label class=col-form-label>Telefono</label>
                <input class="form-control Telefono" type="text" name="Telefono" />
            </div>

            <div class="form-group">
                <label>Direccion</label>
                <input class="form-control Direccion"  type="text" name="Direccion" />
            </div>
            <div class="form-group">
                <label>Cedula</label>
                <input class="form-control Cedula" type="text" name="Cedula" />
            </div>
            <div class="form-group">
                <label class=col-form-label>Genero</label>
                <input type="checkbox" id="femenino" name="Genero" value="1">
                <label for="femenino"> Femenino</label><br>
                <input type="checkbox" id="masculino" name="Genero" value="0">
                <label for="masculino"> Masculino</label><br>
            </div>

            <div class="form-group">
                <!--input type="submit" value="Guardar" class="btn btn-success" /-->
                <!-- Esto es un comentario -->
                <a id="btnGuardar" href="javascript:void(0);" class="btn btn-success">Guardar</a>
            </div>

        </form>
    </div>



</div>

<div class="contenedor"></div>

@section Scripts{
    <script src=~/js/Cliente.js></script>

   


     
}
